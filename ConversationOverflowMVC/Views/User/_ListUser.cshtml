@using Models.Classes;

@foreach (User user in Model)
{
    <tr style="cursor:pointer;">
        <td style="vertical-align: middle; text-align:center;"><i class="fa fa-user"></i></td>
        <td style="vertical-align: middle; text-align:center; font-size:11px;">@user.FirstName @user.LastName</td>
        <td style="vertical-align: middle; text-align:center;"><a href="mailto:@user.Email" />@user.Email</td>
    </tr>
}